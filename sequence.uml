@startuml
actor admin

admin -> "main" : filename
"main" -> "Reader()" : filename
"Reader()" -> "Config" : creates

"Config" -> "main" : is returned

"main" -> "Server()" : Config


Alice -> "Bob()" : Hello
"Bob()" -> "This is very\nlong" as Long
